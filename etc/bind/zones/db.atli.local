;
; BIND data file for local loopback interface
;
$TTL	604800
atli.local.	IN	SOA	server1.atli.local. admin.atli.local. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
; name servers - NS records
	IN	NS	server1.atli.local.

; name servers - A records
server1.atli.local.	IN	A	192.168.100.1
www.atli.local.		in	A	192.168.100.1
lists.atli.local.	IN	A	192.168.100.1
